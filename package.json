{
  "name": "vue-ssr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vite:dev": "vite",
    "dev": "node start-server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --mode uat --ssrManifest --outDir dist/client",
    "build:server": "vite build --mode uat --ssr src/entry-server.js --outDir dist/server",
    "serve": "cross-env NODE_ENV=production node start-server",
    "test": "vitest",
    "lint": "pnpm lint:code",
    "lint:code": "eslint --ext .js,.jsx,.ts,.tsx --fix --quiet ./",
    "build:start-server": "vite build --config vite.server.config.ts",
    "build:rc": "rollup -c",
    "start:dist": "cross-env NODE_ENV=production node rc-start-server.js"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "axios": "^1.5.1",
    "compression": "^1.7.4",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "express": "^4.18.1",
    "jsdom": "^24.1.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "serve-static": "^1.15.0",
    "vant": "^4.7.2",
    "vite-plugin-ssr": "^0.4.142",
    "vue": "^3.3.6",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.8.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/express": "^4.17.13",
    "@types/node": "^18.18.6",
    "@types/nprogress": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.16",
    "cnjm-postcss-px-to-viewport": "^1.0.0",
    "commitlint": "^17.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.18.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^13.1.0",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "puppeteer": "^19.5.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.18.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.2",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.5.0",
    "vite-plugin-cdn2": "^0.15.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-mock-dev-server": "^0.3.21",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitest": "^0.27.2",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.20"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": [
      "npm run lint"
    ]
  }
}
